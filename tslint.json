{
    "defaultSeverity": "error",
    "extends": ["tslint:latest", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-leading-underscore"
      ],
      "curly": [true, "as-needed"],
      "interface-name": [true, "never-prefix"]
    },
    "rulesDirectory": []
  }
  